<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="RegressionSuite" verbose="1" parallel="tests"
	thread-count="4">
	<listeners>
		<listener class-name="com.xyz.autobase.listeners.TestListener" />
		<listener class-name="com.xyz.autobase.listeners.ReportListener" />
	</listeners>
	<test name="PollPositiveTests">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.xyz.tests.TOIPollTests" />
		</classes>
	</test>
</suite>